#!/bin/sh

source ~/GitHub/conf-files/bin/node-remove
source ~/GitHub/conf-files/bin/shell-reset

if [ -d ~/GitHub/oh-my-zsh ]; then
  echo 'removing oh-my-zsh'
  rm -rf ~/GitHub/oh-my-zsh
fi

